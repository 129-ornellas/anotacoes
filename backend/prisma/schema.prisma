// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = "file:./dev.db"
}

model Annotation {
  id         Int       @id @default(autoincrement())
  title      String
  category   String
  remind_in  DateTime
  deleted_at DateTime?
  created_at DateTime
}
